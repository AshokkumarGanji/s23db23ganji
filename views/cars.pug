extends layout

block content
  h1= title
  - var c= results;
  div(class="container-fluid")
  div(class="row")
   each f,index  in c
    div(class="col-sm-2" onclick =`buy(${index})`) #{f.car_model}, #{f.car_year}, #{f.car_price}
    script.
        function buy(a)
        {
            alert(`You purchased Item ${a+1}`)
        } 